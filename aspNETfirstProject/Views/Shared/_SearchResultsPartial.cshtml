@model IEnumerable<aspNETfirstProject.Models.Item>

<h2>Search Results:</h2>

@if (Model.Count() == 0) { @("No Results Found!")}

@foreach (var item in Model)
{
            <a href="/Items/@(item.ItemType + "s")/#@item.ItemType@item.ID ">Title: @item.Title</a>
            <br />
            <span class="description">Description: @item.Description</span>
            <br /><hr>
}



