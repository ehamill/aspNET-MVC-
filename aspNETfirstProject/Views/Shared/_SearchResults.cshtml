@model IEnumerable<aspNETfirstProject.Models.State>

<h2>Search Results:</h2>

@if (Model.Count() == 0)
{ @("No Results Found!")}

@foreach (var state in Model)
{
    <span>State: </span> @state.Name
            <br />
            <span class="description">Description: @state.Abbreviation</span>
            
}
